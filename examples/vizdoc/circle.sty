Point p 
with Circ e {
    p.shape = Circle{
        x: e.shape.x
        y: e.shape.x
        r: 4.0
        color: Colors.black
    }
    p.text = Text{
    string: p.label
    x : p.shape.x
    y: p.shape.y - 20.0
    }
    LOCAL.layering = p.shape above e.shape
    LOCAL.layering2 = p.text above e.shape
}

